{
  "name": "trillo",
  "version": "1.0.0",
  "description": "Trillo application",
  "browserslist": "last 10 versions",
  "main": "index.js",
  "scripts": {
    "start": "sass --watch sass/main.scss:css/style.css",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss css/style.style.comp.css --use autoprefixer -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/compressed-style.css --style=compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Nick Mullenmeister",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0"
  }
}
